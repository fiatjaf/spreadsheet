.coup-de-grace
  /* http://www.colourlovers.com/palette/1098589/coup_de_gr√¢ce */
  $cell-border = #99B898
  $selected-border-color = #E84A5F
  --cell-border: $cell-border
  --dark-cell-border: darken($cell-border, 10)
  --selected-border-color: $selected-border-color
  --selected-background-color: alpha($selected-border-color, 0.1)
  --dependency-border-color: #FECEA8
  --input-background-color: #FF847C
  --selectarea-background-color: alpha(#2A363B, 0.2)
  --handlearea-background-color: lighten($cell-border, 25)

